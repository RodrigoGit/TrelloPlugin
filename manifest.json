{
"manifest_version": 2,
"name": "Trello Plugin",
"description": "link para abrir o trello",
"version": "1.1",
"browser_action": {
   "default_icon": "/image/trello.png",
   "default_popup": "index.html"
},
 "background": {
    "scripts": ["/js/background.js"],
    "persistent": false
},
"permissions": [
  "activeTab",
  "tabs",
  "*://*/*"
]
}